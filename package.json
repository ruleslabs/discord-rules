{
  "name": "@rulesorg/discord-rules",
  "version": "0.1.21",
  "description": "A set of functions to interact with Rules discord guild",
  "scripts": {
    "build": "tsup && yarn build:esm && yarn build:dts",
    "build:esm": "tsup --clean false --format esm --platform node",
    "build:dts": "tsup --clean false --dts-only",
    "prepublishOnly": "yarn build"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruleslabs/discord-rules.git"
  },
  "author": "Charles Lanier",
  "license": "MIT",
  "devDependencies": {
    "tsup": "6.7.0",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "bugs": {
    "url": "https://github.com/ruleslabs/discord-rules/issues"
  },
  "homepage": "https://github.com/ruleslabs/discord-rules#readme",
  "dependencies": {}
}
